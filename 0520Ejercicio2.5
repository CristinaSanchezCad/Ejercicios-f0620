package examen.ejercicio2;

public class ImporteMensualException extends RuntimeException {

    private static final long serialVersionUID = 1L;

	public ImporteMensualException() {
    }

    public ImporteMensualException(String message) {
        super(message);
    }

    public ImporteMensualException(String message, Throwable cause) {
        super(message, cause);
    }

    public ImporteMensualException(Throwable cause) {
        super(cause);
    }

    public ImporteMensualException(String message, Throwable cause, boolean enableSuppression, boolean writableStackTrace) {
        super(message, cause, enableSuppression, writableStackTrace);
    }
    
}
